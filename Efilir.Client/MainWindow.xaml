<Window
    x:Class="Efilir.Client.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="929"
    Height="699"
    mc:Ignorable="d">
    <Grid>
        <Image
            x:Name="ImageView"
            Width="600"
            Height="600"
            Margin="10,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top" />
        <Button
            Margin="0,35,240,0"
            VerticalAlignment="Top"
            Click="Start_ButtonClick"
            Content="Start" HorizontalAlignment="Right" Width="74" />
        <ListBox
            Name="CellData"
            Height="199"
            Margin="0,60,42,0"
            VerticalAlignment="Top" HorizontalAlignment="Right" Width="272" />
        <Button
            Margin="0,35,141,0"
            VerticalAlignment="Top"
            Click="PauseButton_Click"
            Content="Pause" HorizontalAlignment="Right" Width="74" />
    </Grid>
</Window>